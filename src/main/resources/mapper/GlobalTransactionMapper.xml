<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.mapper.GlobalTransactionMapper">
    <select id="queryAllTransactionStatus" resultType="map">
        select id,status from global_transaction_table where status in (0,1);
    </select>
</mapper>